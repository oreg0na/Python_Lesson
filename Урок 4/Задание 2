n1 = int(input("Введите первое целое число: "))
n2 = int(input("Введите второе целое число: "))
print(n1, 'умноженное на', n2, 'равно', n1*n2)
print(n1, 'деленное на', n2, 'равно', n1/n2)
print(n1, 'нацело деленное на', n2, 'равно', n1//n2)
print('Остаток от деления', n1, 'на', n2, 'равно', n1 % n2)
print(n1, 'в степени', n2, 'равно', n1**n2)
